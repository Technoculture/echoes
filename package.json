{
  "name": "echoes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.(ts|tsx)\"",
    "format-check": "prettier --check \"**/*.(ts|tsx)\"",
    "type-check": "tsc --pretty --noEmit",
    "migrate:gen": "drizzle-kit generate:mysql --out src/lib/db/migrations --schema src/lib/db/schema.ts",
    "migrate:genlite": "drizzle-kit generate:sqlite --out src/lib/db/miglite --schema src/lib/db/schema.ts",
    "migrate:push": "node -r esbuild-register src/lib/db/migrate.ts",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.19.0",
    "@clerk/themes": "^1.7.4",
    "@dqbd/tiktoken": "^1.0.3",
    "@libsql/client": "^0.3.1",
    "@phosphor-icons/react": "^2.0.10",
    "@planetscale/database": "^1.7.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@t3-oss/env-core": "^0.3.1",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@uploadthing/react": "^5.2.0",
    "@upstash/redis": "^1.20.2",
    "ai": "^2.1.13",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "drizzle-orm": "^0.26.5",
    "drizzle-zod": "^0.4.2",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "iconoir-react": "^6.8.0",
    "langchain": "^0.0.92",
    "lucide-react": "^0.224.0",
    "next": "^13.4.4",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-textarea-autosize": "^8.4.1",
    "remark-gfm": "^3.0.1",
    "tailwind-merge": "^1.12.0",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "5.0.3",
    "uploadthing": "^5.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "^15.5.7",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "drizzle-kit": "^0.18.1",
    "esbuild-register": "^3.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1"
  }
}
